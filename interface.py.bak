#!/usr/bin/env python3

import subprocess
import os

while True:

    userinput = input("[blueshell]$ ")

    if(userinput == "exit"):
        print("[*] Exiting interface... ")
	exit()

    if(userinput == "deploy"):
        if(os.path.isfile("python_deploy.py")):
            print("[*] Running deploy script...")
            os.system("python_deploy.py")

        else:
            print("[-] Deploy Script not found")
